<%- include('header'); -%>
	<body>
		<div class="box" id="#heading">
			<h1> <%= dayName %> </h1>
		</div>
		<div class="box">
			<% for (var i = 0; i<listItems.length; i++) { %>
				<div class="item">
					<input id="option1" class="regular-checkbox" type="checkbox" name="" value="">
					<p> <%= listItems[i] %> </p>
				</div>
				<% } %>
			</div>
			
			<form  autocomplete="off" class="input" action="" method="post" accept-charset="utf-8">
				<input type="text" name="listItem" placeholder="new item" required>
				<button type="submit">+</button>
			</form>
			
			
		</body>
		<%- include('footer'); -%>